<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Labbay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.Labbay.Chat" />
        <activity android:name="com.Labbay.My_Reviews" />
        <activity android:name="com.Labbay.Send_request" />
        <activity android:name="com.Labbay.Withdrawal_Balance" />
        <activity android:name="com.Labbay.Transaction" />
        <activity android:name="com.Labbay.Performance" />
        <activity android:name="com.Labbay.Share_and_Earn" />
        <activity android:name="com.Labbay.New_Deals" />
        <activity android:name="com.Labbay.Market" />
        <activity android:name="com.Labbay.Awarded" />
        <activity android:name="com.Labbay.OngoingRequest2" />
        <activity android:name="com.Labbay.SplashScreen" />
        <activity android:name="com.Labbay.Summary" />
        <activity android:name="com.Labbay.RegisterScreen" />
        <activity android:name="com.Labbay.LoginScreen" />
        <activity android:name="com.Labbay.MainActivity" />


        <activity android:name="com.Labbay.OnGoingRequest">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>